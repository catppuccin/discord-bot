{
	"name": "catppuccin-pepperjack",
	"version": "1.0.0",
	"author": "Catppuccin",
	"description": "Utility Discord bot for the Catppucin server.",
	"keywords": [
		"catppuccin",
		"discord",
		"utility",
		"bot"
	],
	"license": "MIT",
	"type": "module",
	"private": true,
	"main": "build/main.js",
	"scripts": {
		"prepare": "husky install",
		"prettier": "prettier --write src/",
		"eslint": "eslint src/ --ext .ts",
		"prebuild": "npm run eslint && npm run prettier",
		"build": "tsc",
		"dev": "node --loader ts-node/esm/transpile-only src/main.ts",
		"serve": "node build/main.js",
		"start": "nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts"
	},
	"lint-staged": {
		"*.ts": ["prettier --write", "eslint"],
		"*.md": ["prettier --write"]
	},
	"dependencies": {
		"@discordx/importer": "^1.1.9",
		"@discordx/pagination": "^2.1.2",
		"@octokit/rest": "^19.0.3",
		"@prisma/client": "^4.0.0",
		"discord-api-types": "^0.30.0",
		"discord.js": "^13.7.0",
		"discordx": "^9.6.0",
		"dotenv": "^16.0.1",
		"fuse.js": "^6.6.2",
		"reflect-metadata": "^0.1.13"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.0.3",
		"@types/node": "^17.0.33",
		"@typescript-eslint/eslint-plugin": "5.30.6",
		"@typescript-eslint/parser": "5.30.6",
		"eslint": "8.19.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"nodemon": "^2.0.16",
		"prettier": "2.7.1",
		"prisma": "^4.0.0",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.4"
	},
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=7.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/catppuccin/discord-bot.git"
	},
	"bugs": {
		"url": "https://github.com/catppuccin/discord-bot/issues"
	},
	"homepage": "https://github.com/catppuccin/discord-bot#readme"
}
