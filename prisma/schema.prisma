// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Channel {
  id           Int       @id
  cid          Int       @default(0)
  hthresh      Int
  horni        Boolean   @default(true)
  autothread   Boolean   @default(false)
}
